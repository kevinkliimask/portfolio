{
  "rewrites": [
    {
      "source": "/**/",
      "destination": "/$1/index.html"
    },
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/**",
      "headers": [{
        "key": "Cache-Control",
        "value": "no-cache, no-store, must-revalidate"
      }]
    }
  ]
} 